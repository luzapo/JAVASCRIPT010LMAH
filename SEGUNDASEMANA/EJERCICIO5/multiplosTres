'use strict'
/**
 * Leer los primeros 50 números y contar cuantos múltiplos de 3 hay, imprimir 
 * la cantidad
 * 
 */

let i = 1;
let multiplo = 0;
while (i <= 50) {
    if (i % 3 === 0) {
        multiplo = multiplo + 1;
    }
    i++;
}
console.log('Cantidad de números múltiplos de 3: ' + multiplo);